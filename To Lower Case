class Solution {
    public String toLowerCase(String str) {
        char [] tolowerstr = str.toCharArray();
        StringBuilder st = new StringBuilder();
        for(int i=0;i<tolowerstr.length;i++){
            char c = Character.toLowerCase(tolowerstr[i]);
            st.append(c);
        }
        String s = st.toString();
        return s;
    }
}
